{
  "name": "@unoperate/golem-vaddr-cli",
  "version": "0.1.10",
  "description": "Vanity address generator CLI",
  "bin": {
    "golem-addr": "dist/index.js"
  },
  "exports": {
    ".": {
      "default": "./dist/index.js"
    },
    "./lib": {
      "default": "./dist/lib.js"
    }
  },
  "scripts": {
    "prebuild": "ts-node tools/update-version.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "generate": "ts-node ../tools/generate.ts",
    "dev": "ts-node src/index.ts generate",
    "crunch": "ts-node src/crunch.ts",
    "format": "prettier . --check",
    "format:fix": "prettier . --write",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "jest",
    "test:watch": "jest --watch",
    "list-cpu-offers": "node forward.js golem-sdk market scan",
    "list-gpu-offers": "node forward.js golem-sdk market scan --engine vm-nvidia",
    "db:setup": "npx drizzle-kit generate && npx drizzle-kit migrate",
    "db:clear": "rm -r drizzle/* db.sqlite",
    "db:studio": "npx drizzle-kit studio"
  },
  "keywords": [
    "vanity",
    "address",
    "generator",
    "cli"
  ],
  "author": "",
  "license": "GPL-3.0",
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@golem-sdk/cli": "^2.11",
    "@types/elliptic": "^6.4.18",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@types/ws": "^8.18.1",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9.28.0",
    "jest": "^29.5.0",
    "prettier": "3.5.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.1"
  },
  "dependencies": {
    "@golem-sdk/golem-js": "^3.7.0",
    "@golem-sdk/pino-logger": "^1.1.0",
    "@libsql/client": "^0.15.9",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.60.1",
    "@opentelemetry/exporter-metrics-otlp-http": "^0.202.0",
    "@opentelemetry/exporter-prometheus": "^0.203.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.202.0",
    "@opentelemetry/resources": "^2.0.1",
    "@opentelemetry/sdk-logs": "^0.202.0",
    "@opentelemetry/sdk-metrics": "^2.0.1",
    "@opentelemetry/sdk-node": "^0.202.0",
    "@opentelemetry/sdk-trace-base": "^2.0.1",
    "@opentelemetry/sdk-trace-node": "^2.0.1",
    "@opentelemetry/semantic-conventions": "^1.34.0",
    "@types/uuid": "^10.0.0",
    "cli-progress": "^3.12.0",
    "commander": "^11.0.0",
    "dotenv": "^16.5.0",
    "drizzle-orm": "^0.44.4",
    "elliptic": "^6.6.1",
    "ethers": "^6.14.3",
    "js-sha3": "^0.9.3",
    "pino": "^9.7.0",
    "pino-opentelemetry-transport": "^1.0.1",
    "rxjs": "^7.8.2",
    "uuid": "^11.1.0",
    "zod": "^3.25.76"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.test.ts"
    ]
  },
  "overrides": {
    "@golem-sdk/golem-js": "$@golem-sdk/golem-js"
  }
}
